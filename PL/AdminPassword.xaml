<Window x:Class="PL.AdminPassWord"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PL"
        mc:Ignorable="d"
        Title="AdminPassWord" Height="188" Width="260">
    <StackPanel HorizontalAlignment="Center" Margin="0,40,0,0" Width="254">
        <Label x:Name="message" Content="Plese enter Password to entry as Admin:" FontFamily="Tahoma"  Width="230" HorizontalAlignment="Left" Margin="10,0,10,0" Padding="5,5,5,5" Height="25" VerticalAlignment="Bottom"/>
        <PasswordBox x:Name="AdminPassword" Width="230" HorizontalAlignment="Left"
        Margin="10,5,10,0" BorderBrush="Gray" BorderThickness="1" Password="" Height="20"/>
        <Button x:Name="Chack" Content="Chack" Width="230" Height="25" Margin="10,5,10,0" BorderBrush="Gray" BorderThickness="2"
                Background="Green" Foreground="White" HorizontalAlignment="Left" Click="Chack_Click" />
        <Label x:Name="password_for_teacher" Content="password for tester: 123456" Margin="10,0,0,0" />
    </StackPanel>
</Window>
